{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ff479c",
          "foreground": "#ffffff",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "agnoster_full",
            "mapped_locations": {
              "C:/Projects/": "\ue65d",
              "D:/Projects/": "\ue65d"
            }
          },
          "style": "plain",
          "template": "<background,transparent>\ue0b6</> {{ .Path }} <background>\ue0b0</>",
          "type": "path"
        },
        {
          "background": "#fffb38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#193549",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_user": true
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> {{ .UpstreamIcon }}{{ .HEAD }}{{if not .User.Email }} \udb84\udfe4{{end}}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} <background>\ue0b0</>",
          "type": "git"
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#000000",
          "background": "#01c4c4",
          "template": "<parentBackground>\ue0b0</>{{ if .Unsupported }}\uf071{{ else }} \ue77f {{ .Full }} {{ end }}<background>\ue0b0</>"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} <background>\ue0b0</>",
          "type": "node"
        },
        {
          "background": "#8ED1F7",
          "foreground": "#111111",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} <background>\ue0b0</>",
          "type": "go"
        },
        {
          "background": "#4063D8",
          "foreground": "#111111",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<parentBackground\ue0b0</> \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} <background>\ue0b0</>",
          "type": "julia"
        },
        {
          "background": "#FFDE57",
          "foreground": "#111111",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} <background>\ue0b0</>",
          "type": "python"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} <background>\ue0b0</>",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} <background?>\ue0b0</>",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "display_default": false
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} <background>\ue0b0</>",
          "type": "aws"
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \uf0ad <background>\ue0b0</>",
          "type": "root"
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> \uf520 {{ .FormattedMs }}<background>\ue0b0</>",
          "type": "executiontime"
        },
        {
          "background": "#c386f1",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain",
          "template": "{{if or .Env.POSH_JOBS_RUNNING .Env.POSH_JOBS_FINISHED }}<parentBackground>\ue0b0</> \udb86\udd35 {{ default 0 .Env.POSH_JOBS_RUNNING }} \udb86\udd37 {{ default 0 .Env.POSH_JOBS_FINISHED }}<background>\ue0b0</>{{ end }}"
        },
        {
          "background": "#00897b",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "<parentBackground>\ue0b0</> {{ if eq .Code 0 }}\uf00c{{ else }}\uf00d{{ end }} <background,transparent>\ue0b4</>",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<background,transparent>\ue0b6</> \uf489 <background>\ue0b2</>",
          "type": "shell"
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "charged_icon": "\uead0 ",
            "charging_icon": "\u26a1 ",
            "discharging_icon": "\uf240 "
          },
          "style": "plain",
          "template": "<background,parentBackground>\ue0b2</> {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}%<background>\ue0b2</>",
          "type": "battery"
        },
        {
          "background": "#2e9599",
          "foreground": "#111111",
          "style": "plain",
          "template": "<background,parentBackground>\ue0b2</> {{ .CurrentDate | date .Format }} <background,transparent>\ue0b4</>",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
